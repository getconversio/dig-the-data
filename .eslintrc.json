{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "d3": true,
    "$": true
  },
  "rules": {
    "no-plusplus": "off",
    "strict": "off",
    "comma-dangle": "off",
    "arrow-parens": "off",
    "func-names": "off",
    "space-before-function-paren": "off",
    "no-param-reassign": "off"
  }
}
